{
  "name": "@swissjs/swite",
  "version": "0.1.0",
  "description": "SWITE - SWISS Development Server (Vite replacement)",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -b",
    "dev": "tsc -b --watch",
    "clean": "rm -rf dist node_modules && rm -f tsconfig.tsbuildinfo",
    "generate-import-map": "tsx src/utils/generate-import-map-cli.ts",
    "test": "node --import tsx --test __tests__/import-rewriter-bug.test.ts"
  },
  "dependencies": {
    "@swissjs/core": "workspace:*",
    "@swissjs/compiler": "workspace:*",
    "@swissjs/plugin-file-router": "workspace:*",
    "chalk": "^5.3.0",
    "chokidar": "^3.5.3",
    "es-module-lexer": "^1.3.1",
    "esbuild": "^0.21.5",
    "express": "^4.18.2",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "18.19.121",
    "@types/ws": "^8.5.8",
    "tsx": "^4.21.0",
    "typescript": "^5.0.0"
  },
  "license": "MIT"
}
